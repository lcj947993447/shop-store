<!-- packageShop/pages/images/index.wxml -->
<view class="wrap">
  <view class="title">图片管理</view>
  <view class="image-list">
    <view class="image-item" wx:for="{{images}}" wx:key="index">
      <image class="image" src="{{item}}" mode="widthFix" />
      <view class="icon-wrap">
        <view class="icon iconfont icon-del"></view>
      </view>
    </view>
  </view>
  <!-- <view class="add-image button" bindtap="upload">上传图片</view> -->
</view>