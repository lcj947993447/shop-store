<!-- custom-tab-bar/index.wxml -->
<view class="tab-bar">
  <view wx:for="{{list}}" wx:key="index" class="tab-bar-item {{selected === index ? 'active' : ''}}" data-path="{{item.pagePath}}" bindtap="switchTab">
    <view class="icon iconfont {{item.icon}} "></view>
    <view class="text">{{item.text}}</view>
  </view>
</view>